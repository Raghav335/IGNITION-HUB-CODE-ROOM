#include<stdio.h>
int main()
{
int a,b,c,ch;
    printf("1 for add, 2 for sub, 3 for multiplication,4for devide");
    printf("Enter two numbers");
    scanf("%d",&a,&b);
    printf("Enter choice");
    scanf("%d",&ch);
    switch(ch)
    {
    case 1:
    c=a+b;
    printf("Addition=%d",c);
    break;
    case 2:
    c=a-b;
    printf("sub=%d",c);
    break;
    case 3:
    c=a*b;
    printf("multiplication=%d",c);
    break;
    case 4:
    c=a/b;
    printf("division=%d",c);
    break;
    default:
    printf("wrong input");
    }
    return 0;
}
