#include<stdio.h>
int main (){
              int i,j,a[50][50],r,c,b[50][50] , multiply [50][50];
              printf("enter the rows no. and coloum no.");
              scanf("%d%d",&r,&c);
              printf("Enter the array elements ");
               for(i=0;i<r;i++)
               {
                  for (j=0;j<c;j++)
                   {
                     scanf("%d",&a[i][j]);
                   }
               }
               printf("printer array\n");
                for(i=0;i<r;i++)
                   {
                       for(j=0;j<c;j++)
                       {
                         printf("%d",a[i][j]);
                       }
                       printf("\n");
                   }
                printf("Enter the second matric");
                printf("\nenter the second array elements ");
                for(i=0;i<r;i++)
               {
                  for (j=0;j<c;j++)
                   {
                     scanf("%d",&b[i][j]);
                   }
               }
               printf("printer array\n");
                for(i=0;i<r;i++)
                   {
                       for(j=0;j<c;j++)
                       {
                         printf("%d",b[i][j]);
                       }
                       printf("\n");
                   }
                  printf("array maticx multiply\n");
                  for(i=0;i<r;i++)
                   {
                       for(j=0;j<c;j++)
                       {
                         multiply [i][j]=a[i][j]*b[i][j];
                       
                       }
                       
                       
                  }
                    for(i=0;i<r;i++)
                   {
                       for(j=0;j<c;j++)
                       {
                         printf("%d",multiply [i][j]);
                       
                       }
                       
                       printf("\n");
                  }
 
 return 0;
}
